{
  "name": "runescriptts",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "antlr:lexer": "bunx antlr4ts-cli src/antlr/RuneScriptLexer.g4",
    "antlr:parser": "bunx antlr4ts-cli -visitor -no-listener src/antlr/RuneScriptParser.g4",
    "build": "bunx tsc",
    "clean": "rm -rf dist",
    "prepare": "bun run antlr:lexer && bun run antlr:parser"
  },
  "dependencies": {
    "antlr4ts": "^0.5.0-alpha.4"
  },
  "devDependencies": {
    "@iarna/toml": "2.2.5",
    "@types/node": "^25.2.3",
    "antlr4ts-cli": "^0.5.0-alpha.4",
    "typescript": "^5.3.0"
  }
}
