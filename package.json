{
  "name": "runescriptts",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "antlr": "bunx antlr4ts-cli -visitor -no-listener -o src/antlr/out src/antlr/RuneScriptParser.g4",
    "antlr:lexer": "bunx antlr4ts-cli -o src/antlr/out src/antlr/RuneScriptLexer.g4",
    "build": "tsc",
    "clean": "rm -rf dist",
    "prepare": "bun run antlr && bun run build"
  },
  "dependencies": {
    "antlr4ts": "^0.5.0-alpha.4"
  },
  "devDependencies": {
    "antlr4ts-cli": "^0.5.0-alpha.4",
    "typescript": "^5.3.0",
    "@iarna/toml": "2.2.5"
  }
}
